    <openerp>
        <data>

            <!-- Pediatrics Symptom Checklist Management -->

            <record id="view_oeh_medical_pediatrics_psc" model="ir.ui.view">
                <field name="name">PCS Search</field>
                <field name="model">oeh.medical.pediatrics.psc</field>
                <field name="type">search</field>
                <field name="arch" type="xml">
                    <search string="Search Pediatric Symptom Checklists">
                        <separator orientation="vertical"/>
                            <field name="name" select="1"/>
                            <field name="patient" string="Patient" select="1"/>
                            <field name="doctor" string="Physician" select="1"/>
                        <newline/>
                        <group expand="0" string="Group By..." colspan="11" col="11">
                            <filter name="f39" string="Patient" context="{'group_by':'patient'}"/>
                            <filter name="f40" string="Doctor" context="{'group_by':'doctor'}"/>
                        </group>
                    </search>
                </field>
            </record>

